package com.fabrica.sisgene.domain;

import java.util.List;

/**
 *
 * @author ASUS
 */
public class SeccionUtil {
    
    private int seccion;
    private String descripcion;
    private List<SubseccionUtil> listaSubs;

    public int getSeccion() {
        return seccion;
    }

    public void setSeccion(int seccion) {
        this.seccion = seccion;
    }

    public String getDescripcion() {
        return descripcion;
    }

    public void setDescripcion(String descripcion) {
        this.descripcion = descripcion;
    }

    public List<SubseccionUtil> getListaSubs() {
        return listaSubs;
    }

    public void setListaSubs(List<SubseccionUtil> listaSubs) {
        this.listaSubs = listaSubs;
    }

    public SeccionUtil(int seccion, String descripcion, List<SubseccionUtil> listaSubs) {
        this.seccion = seccion;
        this.descripcion = descripcion;
        this.listaSubs = listaSubs;
    }
    
}
